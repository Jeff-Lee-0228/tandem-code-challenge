{
  "name": "tandem-code-challenge",
  "version": "1.0.0",
  "description": "this is tandem code challenge for apprenticeship application",
  "main": "index.js",
  "scripts": {
    "build-client": "webpack -p",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token.js",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha -w \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "author": "Jeff Lee",
  "license": "ISC",
  "babel": {
    "presets": [
      "babel-preset-fsa"
    ]
  },
  "mocha": {
    "require": [
      "@babel/register",
      "jsdom-global/register"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "express": "^4.16.2",
    "prettier": "^2.1.2",
    "react-bootstrap": "^1.4.0",
    "redux-devtools-extension": "^2.13.8"
  },
  "devDependencies": {
    "@babel/register": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babel-preset-fsa": "github:FullstackAcademy/babel-preset-fsa",
    "chai": "^4.2.0",
    "core-js": "^3.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "pgtools": "^0.3.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "supertest": "^4.0.2",
    "webpack": "^3.10.0"
  }
}
